        <managed-bean-name>TimesHeetBean</managed-bean-name>
        <managed-bean-class>Beans.TimesHeetBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>              
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AddUserBean</managed-bean-name>
        <managed-bean-class>Beans.AddUserBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>          
    </managed-bean>
    <managed-bean>
        <managed-bean-name>UserBean</managed-bean-name>
        <managed-bean-class>Beans.UserBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>              
    </managed-bean>
</faces-config>
