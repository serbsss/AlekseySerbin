<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>AddAppProvBean</managed-bean-name>
        <managed-bean-class>Beans.AddAppProvBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>          
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AppProvBean</managed-bean-name>
        <managed-bean-class>Beans.AppProvBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>              
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AddApplicationBean</managed-bean-name>
        <managed-bean-class>Beans.AddApplicationBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>          
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean</managed-bean-name>
        <managed-bean-class>Beans.ApplicationBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>              
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AddMaterialBean</managed-bean-name>
        <managed-bean-class>Beans.AddMaterialBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>          
